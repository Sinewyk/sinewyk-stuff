{
  "name": "sinewyk-stuff",
  "version": "0.1.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config=./webpack.prod",
    "build:test": "cross-env TESTING=true yarn build",
    "analyse": "yarn build && webpack-bundle-analyzer dist/stats.json",
    "analyze": "yarn analyse",
    "serve": "serve dist --listen=8000 --single",
    "serve:test": "yarn build:test && serve dist --listen=8000 --single",
    "prettify": "prettier --write components/** pages/** posts/*.tsx? public/*.ts src/** styles/** declaration.d.ts package.json postcss.config.js webpack*",
    "test:dev": "cypress open",
    "test": "cypress run",
    "ci": "start-server-and-test serve:test http://localhost:8000 test",
    "watch": "webpack-dev-server --hot --config=./webpack.dev"
  },
  "devDependencies": {
    "@types/history": "4.6.2",
    "@types/node": "10.10.3",
    "autoprefixer": "9.1.3",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "2.0.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "cssnano": "4.1.0",
    "cycle-restart": "0.2.3",
    "cypress": "3.1.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-webpack": "3.0.0",
    "markdown-loader": "4.0.0",
    "mini-css-extract-plugin": "0.4.2",
    "postcss-loader": "3.0.0",
    "prettier": "1.14.2",
    "serve": "10.0.0",
    "start-server-and-test": "1.7.11",
    "style-loader": "0.23.0",
    "ts-loader": "5.1.1",
    "typescript": "2.8.4",
    "url-loader": "1.1.1",
    "webpack": "4.17.1",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  },
  "dependencies": {
    "@cycle/dom": "21.0.0",
    "@cycle/history": "6.10.0",
    "@cycle/run": "4.4.0",
    "cyclejs-utils": "3.2.0",
    "normalize.css": "8.0.0",
    "path-to-regexp": "2.3.0",
    "ramda": "0.25.0",
    "snabbdom-pragma": "2.7.0",
    "xstream": "11.7.0"
  },
  "resolutions": {
    "webpack-sources": "1.2.0"
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  }
}
