{
  "name": "sinewyk-stuff",
  "version": "0.1.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config=./webpack.prod",
    "build:test": "cross-env TESTING=true npm run build",
    "analyse": "npm run build && webpack-bundle-analyzer dist/stats.json",
    "analyze": "npm run analyse",
    "serve": "serve dist --listen=8000 --single",
    "format": "prettier --write --list-different \"./**/*.{ts,js,json,md,css}\"",
    "test:dev": "cypress open",
    "test": "npm run test:unit && npm run build:test && start-server-and-test serve http://localhost:8000 test:functional",
    "test:unit": "npm run format && tsc",
    "test:functional": "cypress run",
    "watch": "cross-env NODE_ENV=development webpack serve --hot --config=./webpack.dev"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/history": "^4.7.8",
    "@types/node": "^14.14.33",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.1",
    "cssnano": "^4.1.7",
    "cycle-restart": "^0.2.2",
    "cypress": "^3.4.0",
    "favicons": "^6.2.1",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-webpack": "^5.0.0",
    "markdown-loader": "^6.0.0",
    "mini-css-extract-plugin": "^1.5.0",
    "postcss": "^8.2.7",
    "postcss-loader": "^5.1.0",
    "prettier": "^2.2.1",
    "serve": "10.1.1",
    "start-server-and-test": "^1.12.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.4",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@cycle/dom": "^22.8.0",
    "@cycle/history": "^7.2.0",
    "@cycle/run": "^5.5.0",
    "assert": "^2.0.0",
    "cyclejs-utils": "^4.0.2",
    "normalize.css": "^8.0.0",
    "path-to-regexp": "^3.0.0",
    "process": "^0.11.10",
    "ramda": "^0.26.0",
    "snabbdom": "^0.7.3",
    "xstream": "^11.14.0"
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "browserslist": {
    "production": [
      "> 1%"
    ],
    "development": [
      "last 1 firefox version",
      "last 1 chrome version"
    ]
  }
}
